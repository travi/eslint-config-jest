{
  "name": "@travi/eslint-config-jest",
  "description": "shareable config for ESLint for Jest test files",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "example.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "repository": "travi/eslint-config-jest",
  "bugs": "https://github.com/travi/eslint-config-jest/issues",
  "homepage": "https://npm.im/@travi/eslint-config-jest",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "@travi/eslint-config": "1.0.85",
    "ban-sensitive-files": "1.9.16",
    "commitlint-config-travi": "1.4.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "lockfile-lint": "4.6.2",
    "npm-run-all": "4.1.5",
    "remark-cli": "10.0.0",
    "remark-preset-lint-travi": "2.0.7",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1"
  },
  "dependencies": {
    "@form8ion/eslint-config-jest": "1.0.43"
  }
}
